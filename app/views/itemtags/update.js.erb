<% if flash.now[:notice] %>
    <%= render partial: 'shared/notice' %>
<% else %>
    <%= render partial: 'shared/alert' %>
<% end %>

$('#selectedTags').empty().html("<%= j render partial: 'itemtags/tag_list', locals: {tags: @template_tags, associated: true } %>");
$('#availableTags').empty().html("<%= j render partial: 'itemtags/tag_list', locals: {tags: @available_tags, associated: false } %>");

initializeTagPopovers();