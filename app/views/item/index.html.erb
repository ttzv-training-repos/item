<div>
    <span>Your User ID: <%= @user_id%> (debug info)</span>
</div>

<p>
    User signed in:
</p>
<%= user_signed_in? %>

<p>
    Current User:
</p>
<%= current_user.as_json %>

<p>
    User_session:
</p>
<%= user_session %>

<br>

<p>Log in as:</p>

<% @users.each do |user| %>
    <%= link_to user.email, become_user_path(user.id) %>
    <%= link_to "EDIT", edit_employee_path(user.employee) %>
    <br>
<% end %>


